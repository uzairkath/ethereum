  "source": "pragma solidity ^0.5.11;\n\ncontract Counter {\n    uint256 public count;\n    \n    constructor () public {\n        count = 100;\n    }\n    function increment() public {\n        count++;\n    }\n    function decrement() public {\n        count--;\n    }\n}",
